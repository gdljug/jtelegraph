{
  "description": "This object represents a message. https://core.telegram.org/bots/api#message",
  "type": "object",
  "javaType": "com.roadtonerdvana.jtelegraph.telegrambotapi.types.Message",
  "properties": {
    "message_id": {
      "type": "integer"
    },
    "from": {
      "javaType": "com.roadtonerdvana.jtelegraph.telegrambotapi.types.User",
      "$ref": "user.json",
      "type": "object"
    },
    "date": {
      "type": "integer"
    },
    "chat": {
      "javaType": "com.roadtonerdvana.jtelegraph.telegrambotapi.types.Chat",
      "$ref": "chat.json",
      "type": "object"
    },
    "forward_from": {
      "javaType": "com.roadtonerdvana.jtelegraph.telegrambotapi.types.User",
      "$ref": "user.json",
      "type": "object"
    },
    "forward_from_chat": {
      "javaType": "com.roadtonerdvana.jtelegraph.telegrambotapi.types.Chat",
      "$ref": "chat.json",
      "type": "object"
    },
    "forward_from_message_id": {
      "type": "integer"
    },
    "forward_signature": {
      "type": "string"
    },
    "forward_date": {
      "type": "integer"
    },
    "text": {
      "type": "string"
    },
    "photo": {
      "type": "array",
      "items": {
        "javaType": "com.roadtonerdvana.jtelegraph.telegrambotapi.types.PhotoSize",
        "$ref": "photoSize.json",
        "type": "object"
      } 
    }
  },
  "required": [
    "message_id",
    "date",
    "chat"
  ]
}